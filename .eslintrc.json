{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/vue3-essential",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "rules": {
        "no-tabs":"off",
        "quotes": 0,
        "indent": "off",
        "array-bracket-spacing":"off",
        "semi":"off",
        "dot-notation":"off",
        "comma-dangle":"off"
    }
}
